  <body>
    <header id="home">
      <div class="header-top">
        <div class="email">
          <a href="mailto:Mohitpal606@gmail.com">Mohitpal606@gmail.com</a>
        </div>
        <div class="social">
          <i class="fab fa-facebook"></i>
          <i class="fab fa-twitter"></i>
          <i class="fab fa-youtube"></i>
          <i class="fab fa-instagram"></i>
        </div>
      </div>
      <div class="header-bottom">
        <div class="logo">CARO GO</div>
        <ul class="items">
          <a href="/"><li class="item">home</li></a>
          <a href="/#about"><li class="item">About</li></a>
          <a href="/#shop"><li class="item">Shop</li></a>
          <a href="/contact"><li class="item">Contact</li></a>
        </ul>
        <div class="ham-burger">
          <span></span>
          <span></span>
          <span></span>
          <span></span>
        </div>
      </div>
    </header>

    <form
      action="/register"
      method="post"
      enctype="application/x-www-form-urlencoded"
    >
      <div class="group">
        <input name="name" type="text" /><span class="highlight"></span
        ><span class="bar"></span>
        <label>Name</label>
      </div>
      <div class="group">
        <input name="email" type="email" /><span class="highlight"></span
        ><span class="bar"></span>
        <label>Email</label>
      </div>
      <div class="group">
        <input name="password" type="password" /><span class="highlight"></span
        ><span class="bar"></span>
        <label>Password</label>
      </div>
      <button type="submit" class="button buttonBlue">
        Register
        <div class="ripples buttonRipples">
          <span class="ripplesCircle"></span>
        </div>
      </button>
    </form>

